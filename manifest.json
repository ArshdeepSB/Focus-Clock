{

	"manifest_version": 2, 
	"name": "Lofi Focus Clock", 
	"description": "Helps use lofi music and attention span to give you the best way to study.",
	"version": "0.00.1",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],

			"js": ["content.js", "sketch/p5/p5.js"]
		}

	],
	"background":
	{

	 "scripts": ["background.js", "sketch/sketch.js"]
	}, 
	"browser_action": {

		"default_icon": "lofi.png", 
		"default_popup": "sketch/index.html", 
		"default_title": "Start Studying"

	}

	

}