{

	"manifest_version": 2, 
	"name": "TriCodingTech Organizer Extension", 
	"description": "Organizer Tool",
	"version": "0.00.2",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],

			"js": ["content.js", "sketch/p5/p5.js"]
		}

	],
	"background":
	{

	 "scripts": ["background.js"]
	}, 
	"browser_action": {

		"default_icon": "TCT.png", 
		"default_popup": "sketch/index.html", 
		"default_title": "Make random circles"

	}

	

}